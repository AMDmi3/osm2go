    <scale_min>1</scale_min>
    <scale_max>10000</scale_max>
  </rule>

  <rule>
    <condition k="highway" v="cycleway"/>
    <line colour="#5050fe" colour_bg="#ffffff" dashed="true" width="2" width_bg="3"/>
